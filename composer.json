{
    "name": "faustbrian/eloquent-uuid",
    "description": "Generate UUIDs when saving Eloquent models",
    "keywords": ["laravel", "framework", "Laravel-UUID", "Laravel UUID", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/database": "5.3.* || 5.4.*",
        "illuminate/support": "5.3.* || 5.4.*",
        "ramsey/uuid": "^3.5"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Uuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Uuid\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
