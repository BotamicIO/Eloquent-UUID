{
    "name": "draperstudio/eloquent-uuid",
    "type": "library",
    "description": "Adds UUID support to Eloquent models",
    "keywords": [
        "draperstudio",
        "eloquent-uuid"
    ],
    "homepage": "https://github.com/DraperStudio/eloquent-uuid",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/database": "5.1.* || 5.2.*",
        "ramsey/uuid": "^3.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\Eloquent\\UUID\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\Eloquent\\UUID\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
